{
  "name": "tap-report",
  "version": "0.1.0-alpha.5",
  "description": "A tap reporter",
  "main": "./bin/tap-report",
  "directories": {
    "src": "src",
    "test": "test"
  },
  "scripts": {
    "test": "node test/index.js run-tests | ./bin/tap-report",
    "generate-test-data": "node test/index.js generate-test-data",
    "dev": "nodemon --quiet --exec \"clear && node test/tap-output.js | ./bin/tap-report \"",
    "dev-normal": "nodemon --quiet --exec \"clear && node test/tap-output.js \"",
    "lint": "huel lint --src src",
    "format": "huel format --src src",
    "commitlint": "huel commitmsg",
    "changelog": "huel changelog && git add CHANGELOG.md && git commit -m \"chore: update changelog\""
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "2.3.2",
    "figures": "2.0.0",
    "jsondiffpatch": "0.3.8",
    "please-upgrade-node": "3.0.1",
    "pretty-ms": "3.1.0",
    "tap-parser": "7.0.0"
  },
  "devDependencies": {
    "del": "3.0.0",
    "diff": "3.5.0",
    "huel": "1.0.0-alpha.23",
    "husky": "0.15.0-rc.13",
    "nodemon": "1.17.3",
    "tap": "11.1.3"
  },
  "engines": {
    "node": ">=8.9.0",
    "npm": ">=5.7.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format",
      "pre-push": "npm test",
      "pre-publish": "npm test",
      "commit-msg": "npm run commitlint"
    }
  }
}
